{
  "name": "my-website",
  "version": "1.0.0",
  "repository": "https://github.com/lioneltay/my-website",
  "homepage": "https://app.tekktekk.com",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "s3:view": "open http://lioneltay.com.s3-website-ap-southeast-2.amazonaws.com/",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config ./webpack/webpack.config.prod.js",
    "s3:update": "aws s3 rm s3://lioneltay.com --recursive && aws s3 cp ./dist s3://lioneltay.com --recursive",
    "cloudfront:bust-cache": "aws cloudfront create-invalidation --distribution-id E1MWKWSMZFSYWH --paths /index.html",
    "deploy": "npm run build && npm run s3:update && npm run cloudfront:bust-cache",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}

{
  "name": "tekktekk-notes-client",
  "version": "1.0.0",
  "repository": "https://github.com/lioneltay/tekktekk-notes",
  "homepage": "notes.tekktekk.com",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "APP_MODE=staging webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "dev:local": "APP_MODE=local webpack-dev-server --config ./webpack/webpack.config.dev.js",
    "emulators:start": "firebase use staging && firebase emulators:start --only firestore,functions",
    "build:functions": "cd functions && tsc --watch > /dev/null",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config ./webpack/webpack.config.prod.js",
    "typecheck": "tsc --noEmit --watch --skipLibCheck",
    "lint": "eslint . --ext .ts,.tsx",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "jest:watch": "jest --watch",
    "test:jest": "jest",
    "test:cypress:emulators:exec": "firebase use staging && firebase emulators:exec --only firestore,functions",
    "test:cypress": "npm run test:cypress:emulators:exec -- \"npm run test:cypress:helper\"",
    "test:cypress:emulators:exec:ci": "firebase use staging --token $FIREBASE_DEPLOY_KEY && firebase emulators:exec --only firestore,functions --token $FIREBASE_DEPLOY_KEY",
    "test:cypress:helper": "start-server-and-test dev:local http-get://localhost:7000 cy:run",
    "test:cypress:ci": "npm run test:cypress:emulators:exec:ci -- \"npm run test:cypress:helper\"",
    "test": "concurrently -s all \"npm run test:jest\" \"npm run test:cypress\"",
    "test:ci": "concurrently -s all \"npm run test:jest\" \"npm run test:cypress:ci\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/react": "^10.0.1",
    "@testing-library/user-event": "^10.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^10.17.17",
    "@types/ramda": "^0.25.51",
    "@types/react": "^16.9.23",
    "@types/react-content-loader": "^3.1.4",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "@types/testing-library__cypress": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^4.6.0",
    "cypress": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "firebase-admin": "^8.10.0",
    "firebase-tools": "^7.16.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.1",
    "rimraf": "^2.7.1",
    "start-server-and-test": "^1.10.11",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",

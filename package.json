{
  "name": "tekktekk-notes",
  "version": "1.0.0",
  "repository": "https://github.com/lioneltay/tekktekk-notes",
  "homepage": "https://notes.tekktekk.com",
  "description": "Simple note taking",
  "main": "index.js",
  "scripts": {
    "deploy:dev:dispatch": "gcloud app deploy dispatch.yaml --project=tekktekk --quiet",
    "deploy:dev:client": "cd ./client && npm run deploy:dev && cd ..",
    "deploy:dev:api": "cd ./api && npm run deploy:dev && cd ..",
    "deploy:dev": "npm run deploy:dev:dispatch && npm run deploy:dev:client && npm run deploy:dev:api",
    "test:client": "cd ./client && npm run test && cd ..",
    "test:server": "cd ./server && npm run test && cd ..",
    "test": "npm run test:client && npm run test:server"
  },
